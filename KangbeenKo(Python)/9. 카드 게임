def solution(left, right):
    n=len(left)
    dp = [[0]*(n+1) for _ in range(n+1)]
    for i in range(n-1 , -1, -1):
        for j in range(n-1, -1, -1):
            if left[i]>right[j]:
                dp[i][j]=max(dp[i][j+1]+right[j], dp[i+1][j], dp[i+1][j+1])
            else:
                dp[i][j]=max(dp[i+1][j],dp[i+1][j+1])
    return dp[0][0]
